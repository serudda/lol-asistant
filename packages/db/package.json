{
  "name": "@discord-bot/db",
  "version": "0.1.0",
  "main": "./index.ts",
  "types": "./index.ts",
  "license": "MIT",
  "author": "Indie Creators HQ",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "db:generate": "pnpm with-env prisma generate",
    "db:push": "pnpm with-env prisma db push --skip-generate --accept-data-loss",
    "db:migrate": "pnpm with-env prisma migrate dev",
    "dev": "pnpm with-env prisma studio --port 5556",
    "with-env": "dotenv -e ../../.env.${NODE_ENV:-local} --"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0"
  },
  "devDependencies": {
    "@discord-bot/typescript-config": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.20.0",
    "typescript": "^5.6.2"
  }
}
